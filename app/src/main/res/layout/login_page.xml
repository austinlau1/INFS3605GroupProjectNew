<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"><![CDATA[







    //android:background="#E3EECD"
        android:background="@drawable/back"/>
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

















































    />





























    ]]>

    <ImageView
        android:id="@+id/light_green_background_image"
        android:layout_width="650dp"
        android:layout_height="890dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.874"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.968"
        app:srcCompat="@drawable/light_green_background" />

    <Button
        android:id="@+id/login_button"
        android:layout_width="264dp"
        android:layout_height="45dp"
        android:layout_marginBottom="212dp"
        android:background="@drawable/newbuttondesign"
        android:fontFamily="@font/inter_semibold"
        android:shadowColor="#6A6A6A"
        android:text="LOGIN"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="@color/green3"
        app:strokeColor="#FF5E5E"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/email_input"
        android:layout_width="259dp"
        android:layout_height="50dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="7dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="    Email"
        app:layout_constraintBottom_toTopOf="@+id/pass_input"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/description_text" />

    <EditText
        android:id="@+id/pass_input"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:layout_marginTop="372dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="    Password"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/forgot_button"
        android:layout_width="193dp"
        android:layout_height="17dp"
        android:layout_marginStart="164dp"
        android:layout_marginTop="424dp"
        android:background="#E3EECD"
        android:text="Forgot Password"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/login_title_text"
        android:layout_width="333dp"
        android:layout_height="40dp"
        android:layout_marginTop="224dp"
        android:fontFamily="@font/inter_bold"
        android:gravity="center"
        android:text="Welcome Back!"
        android:textAlignment="center"
        android:textColor="#372D0F"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/description_text"
        android:layout_width="194dp"
        android:layout_height="25dp"
        android:layout_marginTop="1dp"
        android:text="Log in to start exploring"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/email_input"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.576"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_title_text" />

    <TextView
        android:id="@+id/new_user_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="I'm a new user."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.433"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_button"
        app:layout_constraintVertical_bias="0.025" />

    <Button
        android:id="@+id/rego_button"
        android:layout_width="60dp"
        android:layout_height="15dp"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="188dp"
        android:background="@drawable/light_green_background"
        android:fontFamily="@font/inter_semibold"
        android:shadowColor="#6A6A6A"
        android:text="Register"
        android:textColor="#4CAF50"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.647"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_button"
        app:layout_constraintVertical_bias="1.0"
        app:rippleColor="@color/green3"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="270dp"
        android:layout_height="178dp"
        android:layout_marginTop="84dp"
        app:layout_constraintBottom_toTopOf="@+id/email_input"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/three_trees" />

</androidx.constraintlayout.widget.ConstraintLayout>